{
  "name": "mishka-26",
  "private": true,
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "autoprefixer": "10.4.0",
    "browser-sync": "^2.27.10",
    "del": "^6.1.1",
    "gulp": "4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-terser": "^2.1.0",
    "postcss": "^8.4.18",
    "postcss-csso": "^6.0.1",
    "stylelint": "^14.14.0",
    "stylelint-config-recommended-scss": "^7.0.0"
  },
  "scripts": {
    "license": "MIT",
    "editorconfig": "editorconfig-cli",
    "lint": "stylelint \\\"./source/sass/*.scss\\\"",
    "test": "npm run editorconfig && npm run lint",
    "starter": "browser-sync start --server --no-ui --port 8080 --files \"source/\"",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "^16",
    "npm": "8.x"
  },
  "type": "module"
}
